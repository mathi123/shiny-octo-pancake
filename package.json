{
  "name": "share",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "pnpm db:start",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage.enabled true",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --config .prettierrc --check .",
    "format:fix": "prettier --config .prettierrc --write .",
    "prepare": "husky",
    "postinstall": "prisma generate --schema=app/storage/schema.prisma",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:reset": "docker compose down --volumes --remove-orphans && pnpm db:start && pnpm db:migrate",
    "db:migrate": "env $(cat .env.local | xargs) prisma migrate dev --schema=app/storage/schema.prisma"
  },
  "keywords": [],
  "author": "Mathias Colpaert",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/adapter-neon": "^6.16.0",
    "@prisma/adapter-pg": "^6.16.0",
    "@prisma/client": "^6.16.0",
    "next": "^15.5.2",
    "next-auth": "5.0.0-beta.29",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.0",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
